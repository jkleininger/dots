#dmenu
"dmenu_run"
    m:0x40 + c:27
    Mod4 + r 

#st
"st"
    m:0x40 + c:36
    Mod4 + Return 

#lockit
"lockit"
    m:0x40 + c:46
    Mod4 + l 

#lock-and-suspend
"lock-and-suspend"
    m:0x41 + c:46
    Shift+Mod4 + l 

#qutebrowser
"qutebrowser"
    m:0x40 + c:56
    Mod4 + b 

#tp-toggle
"tp-toggle"
    m:0x40 + c:28
    Mod4 + t 

#app finder
"xfce4-appfinder"
    m:0x8 + c:68
    Alt + F2 

#app menu
"xfce4-popup-applicationsmenu"
    m:0x8 + c:67
    Alt + F1 

#toggle decoration
"toggle-decoration"
    m:0x40 + c:40
    Mod4 + d 

